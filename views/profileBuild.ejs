<script>
 var profileEjs = angular.module('profileEjs', []);
   
 			 profileEjs.controller( 'headingProfile', function ($scope,$http,$location){
 			 
   				$scope.editparam = function(){ $http({
   				method: 'PUT',
	            url: '/editUser/heading',
	            data: { "editName": $scope.editName, "editSurName": $scope.editSurName }
	           		
	           		}).success(function(response){           
	            if(response.profile == "edited")
	           		alert("Profile edited Sucessfully!!");
	            else
	            	window.location = '/failureEdit';
	        }).error(function(error){
	        console.log("we are in error!!");
	            alert("error");
	        });   		
 			};
 			
 			$scope.getparam = function(){   
   			console.log("we are in profile Edit");
   				$http({
   				method:'GET',
   				url:'/editUser/getProfileHead',
   				}).success(function(data) {		
			$scope.names= data[0].first_name;
			$scope.surname=data[0].last_name;
				
			}).error(function(response){
		  			console.log("Error response from getparam");
		  			console.log("response");
		  			});
			};
			});

 			profileEjs.controller( 'summaryController', function ($scope,$http,$location){
 			$scope.editsummary = function(){ $http({
   				method: 'PUT',
	            url: '/summary/editsum',
	            data: { "summaryText": $scope.summarytext}
	           		}).success(function(response){	          	            
	            if(response.summary == "edited")
	           		alert("Profile edited Sucessfully!!");
	            else
	            	window.location = '/failureEdit';
	        }).error(function(error){
	        console.log("we are in error!!");
	            alert("error");
	        });   		
 			};
 			
 			$scope.getSummary = function(){   
   			console.log("we are in profile Edit");
   				$http({
   				method:'GET',
   				url:'/summary/getsummary',
   				}).success(function(data) {		
			$scope.summary=data[0].summary;
			}).error(function(response){
		  			console.log("Error response from getparam");
		  			console.log("response");
		  			});
			};
 			});
	
 			profileEjs.controller( 'educationController', function ($scope,$http,$location){
 			$scope.editEducation = function(){ $http({
   				method: 'PUT',
	            url: '/summary/editEdu',
