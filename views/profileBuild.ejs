	        }).error(function(error){
	        console.log("we are in error!!");
	            alert("error");
	        });   		
 			};
 			
 			$scope.getEducation = function(){   
   			console.log("we are in education get");
   				$http({
   				method:'GET',
   				url:'/summary/getEdu',
   				}).success(function(data) {	
   				console.log(data);	
			$scope.school=data[0].school_name;
			$scope.field=data[0].study_field;
			$scope.grades=data[0].grades;
			$scope.description=data[0].education_description;
			$scope.activity=data[0].activities;
			
			}).error(function(response){
		  			console.log("Error response from getparam");
		  			console.log("response");
		  			});
			};
	
 			});
 			
 			
 			
 			profileEjs.controller( 'experienceController', function ($scope,$http,$location){
 			$scope.editExperience = function(){ $http({
   				method: 'PUT',
	            url: '/summary/editExp',
	            data: { "company": $scope.Company, "title":$scope.Title, "location":$scope.Location, "description":$scope.Description}
	           		}).success(function(response){	          	            
	            if(response.experience == "edited")
	           		alert("Experience edited Sucessfully!!");
	            else
	            	window.location = '/failureEdit';
	        }).error(function(error){
	        console.log("we are in error!!");
	            alert("error");
	        });   		
 			};
 			
 			$scope.getExperience = function(){   
   			console.log("we are in experience get");
   				$http({
   				method:'GET',
   				url:'/summary/getExp',
   				}).success(function(data) {	
   				console.log(data);	
			$scope.company=data[0].company_name;
			$scope.title=data[0].title;
			$scope.location=data[0].location;
			$scope.description=data[0].experience_description;
					
			}).error(function(response){
		  			console.log("Error response from getparam");
		  			console.log("response");
		  			});
			};
	
 			});
 			
 		
 			
 			
 			
 			profileEjs.controller( 'searchController', function searchMembers($scope,$http,$location){
  			 console.log("we are in GET members");
   				
   				$scope.searchMembers = function(){ $http({
   				method: 'POST',
	            url: '/searchMembers/getEmailid',
	            data: {"emailid": $scope.member}
	           		
	           		}).success(function(response){
	                 	window.location = '/successGet';
	        }).error(function(error){
	        console.log("we are in error!!");
	            alert("error");
	        });   		
 			};
 			
 			$scope.viewConnections= function(){ $http({
   				method: 'GET',
	            url: '/connections/connectionPage',
	            	           		
	           		}).success(function(response){
	                 	window.location = '/showConnections';
	        }).error(function(error){
	        console.log("we are in error!!");
	            alert("error");
	        });   		
 			};
 			
 			$scope.lastLoginLoad= function(){ $http({
   				method: 'GET',
	            url: '/connections/lastLog',
	            	           		
	           		}).success(function(response){
	           		
	                 	$scope.log=response[0].last_login;
	        }).error(function(error){
	        console.log("we are in error!!");
	            alert("error");
	        });   		
 			};
 			
 			
 			
 			
 			});
 			
 			profileEjs.controller( 'signoutController', function logout($scope,$http,$location){
  			 console.log("we are in GET members");
   				
   				$scope.logout = function(){ $http({
   				method: 'POST',
	            url: '/createUser/logout'
	              		
	           		}).success(function(response){
	           		if(response.status=='logout')
	                 	window.location = '/successLogout';
	                 	
	        }).error(function(error){
	        console.log("we are in error!!");
	            alert("error");
	        });   		
 			};
 			
 			});
 			
 			
 			
</script>







